<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkDialog" id="simple_dialog">
    <property name="title">Simple Dialog</property>
    <property name="default_width">300</property>
    <property name="default_height">100</property>
    <child internal-child="vbox">
      <object class="GtkBox" id="dialog_vbox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="simple_label">
            <property name="label">Simple message</property>
            <property name="visible">true</property>
            <property name="wrap">True</property>
          </object>
        </child>
      </object>
    </child>
    <child type="action">
        <object class="GtkButton" id="ok_button">
        <property name="visible">true</property>
        <property name="label">OK</property>
        </object>
    </child>
    <action-widgets>
        <action-widget response="ok">ok_button</action-widget>
    </action-widgets>
    </object>
</interface>